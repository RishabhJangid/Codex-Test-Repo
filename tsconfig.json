{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "moduleResolution": "Bundler",
    "lib": ["DOM", "ES2020"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "public/dist",
    "rootDir": "src",
    "declaration": false
  },
  "include": ["src/import/**/*.ts"],
  "exclude": ["node_modules"]
}
